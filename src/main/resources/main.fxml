<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefWidth="500" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.antowka.controller.MainController">
   <children>
      <TableView fx:id="properties" prefHeight="124.0" prefWidth="500">
          <columns>
              <TableColumn fx:id="name" prefWidth="100.0" text="name" />
              <TableColumn fx:id="value" prefWidth="400.0" text="value" />
          </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>

       <TableView fx:id="habrTable" layoutY="124.0" prefHeight="576.0" prefWidth="500.0">
           <columns>
               <TableColumn fx:id="link" prefWidth="500.0" text="link" />
           </columns>
           <columnResizePolicy>
               <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
           </columnResizePolicy>
       </TableView>
   </children>
</AnchorPane>
